{
  "name": "onthequad-front-end",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "jade views/index.jade -o public/ && webpack-dev-server --port 5000 --inline --output-public-path javascripts --content-base public/ --history-api-fallback",
    "start:prod": "webpack && node bin/www"
  },
  "author": "William Dickson",
  "repository": {
    "type": "git",
    "url": "git@github.com:onthequad/onthequad-front-end.git"
  },
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.0",
    "compression": "~1.6.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "if-env": "^1.0.0",
    "jade": "^1.11.0",
    "jquery": "^2.1.4",
    "lodash": "^4.6.1",
    "nuka-carousel": "^1.2.0",
    "query-string": "^4.1.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.27.3",
    "react-dom": "^0.14.7",
    "react-dropzone": "^3.4.0",
    "react-loading": "0.0.9",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-router": "~1.0.0-beta7",
    "redux-thunk": "~2.0.1",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "file-loader": "^0.8.5",
    "jade-html-loader": "0.0.3",
    "webpack-dev-server": "^1.14.1",
    "webpack-dev-middleware": "^1.0.11"
  }
}
